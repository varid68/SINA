{
  "extends": "airbnb", // EXISTING
  "parser": "babel-eslint",
  "env": {
    "browser": true
  },
  "plugins": [
    "react"
  ],
  "rules": {
    "no-use-before-define": 0,
    "no-param-reassign": 0,
    "eqeqeq": 0,
    "react/jsx-closing-bracket-location": 0,
    "no-unused-expressions": [
      "error",
      {
        "allowTernary": true
      }
    ],
    "react/jsx-filename-extension": [
      2,
      {
        "extensions": [
          ".js",
          ".jsx"
        ]
      }
    ],
    "react/forbid-prop-types": [
      0
    ],
    "linebreak-style": "off",
    "react/require-default-props": [
      0
    ],
    "global-require": [
      0
    ]
  }
}